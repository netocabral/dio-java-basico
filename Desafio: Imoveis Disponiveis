import java.util.Scanner; 

public class Program {
    public static void main(String[] args) {
        Scanner leitor = new Scanner(System.in);
        String input = leitor.next();
        String[] parts = input.split("/"); // separa a string de entrada pelo caractere "/"
        
        String tipoImovel = parts[0];
        int valor = Integer.parseInt(parts[1]);
        String disponibilidade = parts[2];
        
        System.out.println("Imovel: " + tipoImovel + " R$" + valor + " " + disponibilidade);
    }
}
